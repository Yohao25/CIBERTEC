package vistas;

public class Vista3 {

	public static void main(String[] args) {
		//1. Declaraci√≥n variables
		double vol,rad,alt;
		
		//2. Entrada de datos
		alt=5;
		rad=10;
		//3. Proceso
		vol= 3.14*rad*rad*vol
		//4. Salida de datos
		System.out.println(vol);		
				
	}

}
